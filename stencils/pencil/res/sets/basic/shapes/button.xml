<Shape id="button" displayName="Button" icon="icons/button.png">
  <Properties>
    <PropertyGroup>
      <Property name="box" type="Dimension">240,72</Property>
    </PropertyGroup>
    <PropertyGroup name="Text">
      <Property name="text" displayName="Text" type="PlainText">Button</Property>
    </PropertyGroup>
  </Properties>
  <Behaviors>
    <For ref="ninepatchTC">
      <Box>new Dimension($box.w - 17 - 18, 34)</Box>
    </For>
    <For ref="ninepatchTR">
      <Transform>"translate(" + ($box.w - 17) + ",0)"</Transform>
    </For>

    <For ref="ninepatchCL">
      <Box>new Dimension(18, $box.h - 34 - 37)</Box>
    </For>
    <For ref="ninepatchCC">
      <Box>new Dimension($box.w - 18 - 17,
                         $box.h - 34 - 37)</Box>
    </For>
    <For ref="ninepatchCR">
      <Transform>"translate(" + ($box.w - 17) + ",0)"</Transform>
      <Box>new Dimension(17, $box.h - 34 - 37)</Box>
    </For>

    <For ref="ninepatchBL">
      <Transform>"translate(0," + ($box.h - 37) + ")"</Transform>
    </For>
    <For ref="ninepatchBC">
      <Transform>"translate(0," + ($box.h - 37) + ")"</Transform>
      <Box>new Dimension($box.w - 18 - 17, 37)</Box>
    </For>
    <For ref="ninepatchBR">
      <Transform>"translate(" + ($box.w - 17) + "," +
                                ($box.h - 37) + ")"</Transform>
    </For>

    <For ref="text">
      <TextContent>$text</TextContent>
      <Font>Font.fromString("Droid Sans|normal|normal|21px")</Font>
      <BoxFit>
        <Arg>new Bound(
                 16,
                 10,
                 $box.w - 17 - 16,
                 $box.h - 17 - 10)</Arg>
        <Arg>new Alignment(1, 1)</Arg>
      </BoxFit>
    </For>
  </Behaviors>
  <p:Content xmlns:p="http://www.evolus.vn/Namespace/Pencil" xmlns="http://www.w3.org/2000/svg">
    <image id="ninepatchTL" xlink:href="data:image/png;base64,"
      x="0" y="0" width="18" height="34"/>
    <image id="ninepatchTC" xlink:href="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAAiCAYAAACeLbMRAAAAL0lEQVR4nGNggIJAhi9fvvxn+v//PwNu4u/fvwxMDAwMyGL//v3Dw8LJJVYx3EoAbuZa8MGAnQgAAAAASUVORK5CYII="
      x="18" y="0" height="34"
      preserveAspectRatio="none"/>
    <image id="ninepatchTR" xlink:href="data:image/png;base64,"
      x="0" y="0" width="17" height="34"/>

    <image id="ninepatchCL" xlink:href="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABIAAAABCAYAAADTjP5iAAAAJElEQVR4nGP8//8/AzpgZGTsP3/+fMHfv38Z/v37x4BNDToAANKwD1wWZGGQAAAAAElFTkSuQmCC"
      x="0" y="34" width="18"
      preserveAspectRatio="none"/>
    <image id="ninepatchCC" xlink:href="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAYAAAAfFcSJAAAADUlEQVR4nGM4efLkfwAIFQNbnd/VGQAAAABJRU5ErkJggg=="
      x="18" y="34"
      preserveAspectRatio="none"/>
    <image id="ninepatchCR" xlink:href="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABEAAAABCAYAAAA4u0VhAAAAJ0lEQVR4nGM8efLkfwY8gJGRkYGJiYmBmZmZwdDQcML///8L0dUAADqMB/9CqRkyAAAAAElFTkSuQmCC"
      x="0" y="34" width="17"
      preserveAspectRatio="none"/>

    <image id="ninepatchBL" xlink:href="data:image/png;base64,"
      x="0" y="0" width="18" height="37"/>
    <image id="ninepatchBC" xlink:href="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAAlCAYAAACDKIOpAAAASUlEQVR4nHXMMQrAMAxD0R/HOKfq0bvnUrY6NNBC0+UhgRBzTpkktmTmu1bVficJa61hwCc9uDvWe18pIrAxxg3ACXAgaR38cQHiXzHKT4PyTgAAAABJRU5ErkJggg=="
      x="18" y="0" height="37"
      preserveAspectRatio="none"/>
    <image id="ninepatchBR" xlink:href="data:image/png;base64,"
      x="0" y="0" width="17" height="37"/>
    <text x="16" y="10" id="text" xml:space="preserve"></text>
  </p:Content>
</Shape>
