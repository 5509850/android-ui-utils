<Shape id="editbox" displayName="Edit Field" icon="icons/editbox.png">
  <Properties>
    <PropertyGroup>
      <Property name="box" type="Dimension">350,74</Property>
    </PropertyGroup>
    <PropertyGroup name="Text">
      <Property name="textContent" displayName="Text Content" type="RichText">Edit text</Property>
      <Property name="textFont" displayName="Text Font" type="Font">Droid Sans|normal|normal|27px</Property>
      <Property name="textAlign" displayName="Alignment" type="Alignment">0,1</Property>
    </PropertyGroup>
  </Properties>
  <Behaviors>
    <For ref="ninepatchTC">
      <Box>new Dimension($box.w - 16 - 16, 32)</Box>
    </For>
    <For ref="ninepatchTR">
      <Transform>"translate(" + ($box.w - 16) + ",0)"</Transform>
    </For>

    <For ref="ninepatchCL">
      <Box>new Dimension(16, $box.h - 32 - 39)</Box>
    </For>
    <For ref="ninepatchCC">
      <Box>new Dimension($box.w - 16 - 16,
                         $box.h - 32 - 39)</Box>
    </For>
    <For ref="ninepatchCR">
      <Transform>"translate(" + ($box.w - 16) + ",0)"</Transform>
      <Box>new Dimension(16, $box.h - 32 - 39)</Box>
    </For>

    <For ref="ninepatchBL">
      <Transform>"translate(0," + ($box.h - 39) + ")"</Transform>
    </For>
    <For ref="ninepatchBC">
      <Transform>"translate(0," + ($box.h - 39) + ")"</Transform>
      <Box>new Dimension($box.w - 16 - 16, 39)</Box>
    </For>
    <For ref="ninepatchBR">
      <Transform>"translate(" + ($box.w - 16) + "," +
                                ($box.h - 39) + ")"</Transform>
    </For>

    <For ref="text">
      <TextContent>$textContent</TextContent>
      <Font>$textFont</Font>
      <BoxFit>
        <Arg>new Bound(
                 16,
                 10,
                 $box.w - 16 - 16,
                 $box.h - 17 - 10)</Arg>
        <Arg>$textAlign</Arg>
      </BoxFit>
    </For>
  </Behaviors>
  <p:Content xmlns:p="http://www.evolus.vn/Namespace/Pencil" xmlns="http://www.w3.org/2000/svg">
    <image id="ninepatchTL" xlink:href="data:image/png;base64,"
      x="0" y="0" width="16" height="32"/>
    <image id="ninepatchTC" xlink:href="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAAgCAYAAADT5RIaAAAAP0lEQVR4nJWJsQ3AQAjEzNE8zMA8zPD7L0KaSJHo0liyzczAvXfU3aiqUGaicw6KCOTuSBIysxefrvGrLQC7PQweBxVoq1ZxAAAAAElFTkSuQmCC"
      x="16" y="0" height="32"
      preserveAspectRatio="none"/>
    <image id="ninepatchTR" xlink:href="data:image/png;base64,"
      x="0" y="0" width="16" height="32"/>

    <image id="ninepatchCL" xlink:href="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABAAAAABCAYAAADXeS5fAAAAKElEQVR4nH3BsQ0AIAACMISEL/j/Fb7CydX2bMPTdklgG7YhCSTxcwF83Qbg9e0AtQAAAABJRU5ErkJggg=="
      x="0" y="32" width="16"
      preserveAspectRatio="none"/>
    <image id="ninepatchCC" xlink:href="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAYAAAAfFcSJAAAADUlEQVR4nGP4+vXrfwAJoQPf1gvoUgAAAABJRU5ErkJggg=="
      x="16" y="32"
      preserveAspectRatio="none"/>
    <image id="ninepatchCR" xlink:href="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABAAAAABCAYAAADXeS5fAAAAK0lEQVR4nH3JsQ0AMAwCMPIV/7/CDSxIoZ261qvH9sFHW+wukiAJJIHkvL9IyxL/qlyzagAAAABJRU5ErkJggg=="
      x="0" y="32" width="16"
      preserveAspectRatio="none"/>

    <image id="ninepatchBL" xlink:href="data:image/png;base64,"
      x="0" y="0" width="16" height="39"/>
    <image id="ninepatchBC" xlink:href="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAAnCAYAAADO4CKiAAAAOElEQVR4nK2LIQ7AQAzD3Pz/u1WVK8jgyGloxLKB6e6oqhDwA5Lc7ZZJ0O6icw6yjWYGbIf3+MQD2dorq+wsMaMAAAAASUVORK5CYII="
      x="16" y="0" height="39"
      preserveAspectRatio="none"/>
    <image id="ninepatchBR" xlink:href="data:image/png;base64,"
      x="0" y="0" width="16" height="39"/>

    <foreignObject x="16" y="10" width="200" height="150" id="text">
      <div xmlns="http://www.w3.org/1999/xhtml">
        Edit text content
      </div>
    </foreignObject>
  </p:Content>
</Shape>
