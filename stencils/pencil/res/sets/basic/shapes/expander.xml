<Shape id="expander" displayName="Expander" icon="icons/expander.png">
  <Properties>
    <PropertyGroup>
      <Property name="box" type="Dimension" p:lockHeight="true" p:lockWidth="true">48,54</Property>
      <Property name="expanded" type="Bool" displayName="Expanded">false</Property>
    </PropertyGroup>
  </Properties>
  <Behaviors>
    <For ref="normalImage">
      <Visibility>!$expanded.value</Visibility>
    </For>
    <For ref="expandedImage">
      <Visibility>$expanded</Visibility>
    </For>
  </Behaviors>
  <p:Content xmlns:p="http://www.evolus.vn/Namespace/Pencil" xmlns="http://www.w3.org/2000/svg">
    <image xlink:href="data:image/png;base64,"
      id="normalImage" x="0" y="0" width="48" height="54"/>
    <image xlink:href="data:image/png;base64,"
      id="expandedImage" x="0" y="0" width="48" height="54"/>
  </p:Content>
</Shape>
