<Shape id="button-radio" displayName="Button - Radio" icon="icons/button-radio.png">
  <Properties>
    <PropertyGroup>
      <Property name="box" type="Dimension" p:lockHeight="true" p:lockWidth="true">57,72</Property>
      <Property name="checked" type="Bool" displayName="Checked">false</Property>
    </PropertyGroup>
  </Properties>
  <Behaviors>
    <For ref="offImage">
      <Visibility>!$checked.value</Visibility>
    </For>
    <For ref="onImage">
      <Visibility>$checked</Visibility>
    </For>
  </Behaviors>
  <p:Content xmlns:p="http://www.evolus.vn/Namespace/Pencil" xmlns="http://www.w3.org/2000/svg">
    <image xlink:href="data:image/png;base64,"
      id="offImage" x="0" y="0" width="57" height="72"/>
    <image xlink:href="data:image/png;base64,"
      id="onImage" x="0" y="0" width="57" height="72"/>
  </p:Content>
</Shape>
