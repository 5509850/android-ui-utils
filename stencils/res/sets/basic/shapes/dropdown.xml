<Shape id="dropdown" displayName="Dropdown" icon="icons/dropdown.png">
  <Properties>
    <PropertyGroup>
      <Property name="box" type="Dimension" p:lockHeight="true">350,72</Property>
    </PropertyGroup>
    <PropertyGroup name="Text">
      <Property name="text" displayName="Text" type="PlainText">Selected Item Text</Property>
    </PropertyGroup>
  </Properties>
  <Behaviors>
    <For ref="ninepatchTC">
      <Box>new Dimension($box.w - 89 - 29, 34)</Box>
    </For>
    <For ref="ninepatchTR">
      <Transform>"translate(" + ($box.w - 89) + ",0)"</Transform>
    </For>

    <For ref="ninepatchCL">
      <Box>new Dimension(29, $box.h - 34 - 38)</Box>
    </For>
    <For ref="ninepatchCC">
      <Box>new Dimension($box.w - 29 - 89,
                         $box.h - 34 - 38)</Box>
    </For>
    <For ref="ninepatchCR">
      <Transform>"translate(" + ($box.w - 89) + ",0)"</Transform>
      <Box>new Dimension(89, $box.h - 34 - 38)</Box>
    </For>

    <For ref="ninepatchBL">
      <Transform>"translate(0," + ($box.h - 38) + ")"</Transform>
    </For>
    <For ref="ninepatchBC">
      <Transform>"translate(0," + ($box.h - 38) + ")"</Transform>
      <Box>new Dimension($box.w - 29 - 89, 38)</Box>
    </For>
    <For ref="ninepatchBR">
      <Transform>"translate(" + ($box.w - 89) + "," +
                                ($box.h - 38) + ")"</Transform>
    </For>

    <For ref="text">
      <TextContent>$text</TextContent>
      <Font>Font.fromString("Droid Sans|normal|normal|21px")</Font>
      <BoxFit>
        <Arg>new Bound(
                 16,
                 11,
                 $box.w - 77 - 16,
                 $box.h - 17 - 11)</Arg>
        <Arg>new Alignment(0, 1)</Arg>
      </BoxFit>
    </For>
  </Behaviors>
  <p:Content xmlns:p="http://www.evolus.vn/Namespace/Pencil" xmlns="http://www.w3.org/2000/svg">
    <image id="ninepatchTL" xlink:href="data:image/png;base64,"
      x="0" y="0" width="29" height="34"/>
    <image id="ninepatchTC" xlink:href="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAMAAAAiCAYAAACa2GMsAAAARklEQVR4nGNgwAUY////j+AwMDD4wznfvn2DS7H8+/ePASuHCdk0FmTT8HD+/v1Lsh4UFxCnDNVtRFqK2x6cYUCO24g0GgBwX1gjn3jsnQAAAABJRU5ErkJggg=="
      x="29" y="0" height="34"
      preserveAspectRatio="none"/>
    <image id="ninepatchTR" xlink:href="data:image/png;base64,"
      x="0" y="0" width="89" height="34"/>

    <image id="ninepatchCL" xlink:href="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAB0AAAABCAYAAAAih6XvAAAALUlEQVR4nGNkYGBg+P//PwMDAwMDIyMjj5iYWMfu3buz//79y/Dv3z+4HDUBAJ+/D2JslyBSAAAAAElFTkSuQmCC"
      x="0" y="34" width="29"
      preserveAspectRatio="none"/>
    <image id="ninepatchCC" xlink:href="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAMAAAABCAYAAAAb4BS0AAAAEElEQVR4nGM8ffr0fwYoAAAjPANiTkWQ+AAAAABJRU5ErkJggg=="
      x="29" y="34"
      preserveAspectRatio="none"/>
    <image id="ninepatchCR" xlink:href="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAFkAAAABCAYAAAC11gI1AAAAYElEQVR4nLWOsQnAIBQF39dGcQLHcAh3yiKZyjXEQrARC0HwpU0tydXHcZJSIn4i54wQAlpr2HsfNUiilILeO+acWGt98iYisNbCGAPvPZxzxx2lFLTWiDHetdaL5Hg7DwSAI/+ArIExAAAAAElFTkSuQmCC"
      x="0" y="34" width="89"
      preserveAspectRatio="none"/>

    <image id="ninepatchBL" xlink:href="data:image/png;base64,"
      x="0" y="0" width="29" height="38"/>
    <image id="ninepatchBC" xlink:href="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAMAAAAmCAYAAAABSSE6AAAAWklEQVR4nK3PwQ3AIAgF0A8hMlVH7wLuJPTSqiSYcCgnX/wQoN674y0xs+8Nxlbi7khj8SdgjFGJFacVQUQ54j3HWATzahMROaC1tqCqOQjAveOa2E8Ii/6LB1duMf9Jt0UgAAAAAElFTkSuQmCC"
      x="29" y="0" height="38"
      preserveAspectRatio="none"/>
    <image id="ninepatchBR" xlink:href="data:image/png;base64,"
      x="0" y="0" width="89" height="38"/>
    <text x="16" y="11" id="text" xml:space="preserve"></text>
  </p:Content>
</Shape>
